{
  "name": "react-infini-grid",
  "version": "2.0.8",
  "description": "React base Grid component, adding to the DOM only elements that are in viewport",
  "private": false,
  "main": "index.js",
  "scripts": {
    "start": "node scripts/start.js",
    "build:pages": "node scripts/build.js",
    "test": "node scripts/test.js --setupTestFrameworkScriptFile=raf/polyfill",
    "flow": "flow",
    "flow-typed": "flow-typed",
    "predeploy": "yarn run build:pages",
    "deploy": "gh-pages -d build",
    "build": "yarn build:clean && yarn build:lib && yarn build:flow && yarn build:version",
    "build:clean": "rimraf lib",
    "build:lib": "BABEL_ENV=production babel -d lib src/component --ignore '**/*.test.js'",
    "build:flow": "flow-copy-source -v -i '**/*.test.js' src/component lib",
    "build:version": "npm version patch; cp package.json lib",
    "jest": "jest --clearCache"
  },
  "keywords": [
    "infinity",
    "react",
    "redux",
    "grid",
    "ininity-grid",
    "fast scroll"
  ],
  "author": "Adam Gajzlerowicz",
  "license": "MIT",
  "dependencies": {
    "autoprefixer": "7.1.6",
    "babel-core": "6.26.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.2",
    "babel-preset-react-app": "^3.1.0",
    "babel-runtime": "6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "css-loader": "0.28.7",
    "dotenv": "4.0.0",
    "eslint": "4.10.0",
    "eslint-config-react-app": "^2.0.1",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.39.1",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.4.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.5",
    "flow-typed": "^2.2.3",
    "fs-extra": "^5.0.0",
    "html-webpack-plugin": "2.29.0",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "promise": "8.0.1",
    "raf": "3.4.0",
    "ramda": "^0.25.0",
    "react": "^16.2.0",
    "react-dev-utils": "^4.2.1",
    "react-dom": "^16.2.0",
    "style-loader": "0.19.0",
    "sw-precache-webpack-plugin": "0.11.4",
    "url-loader": "0.6.2",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.4",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "2.0.3"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js",
      "<rootDir>/config/jest/jsdom.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "mjs",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ],
    "env": {
      "production": {
        "presets": [
          "minify"
        ]
      }
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-minify": "^0.2.0",
    "cross-env": "^5.1.3",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "flow-bin": "^0.63.1",
    "flow-copy-source": "^1.2.1",
    "gh-pages": "^1.1.0",
    "jest": "^22.0.6",
    "jsdom": "^11.5.1",
    "react-test-renderer": "^16.2.0",
    "rimraf": "^2.6.2"
  }
}
